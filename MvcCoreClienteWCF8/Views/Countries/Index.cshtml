@model tCountryCodeAndName[]

<ul class="list-group">
    @foreach (var country in Model)
    {
        <li class="list-group-item">
            @country.sName
            <a asp-controller="Countries" asp-action="Details"
                asp-route-isocode="@country.sISOCode"
                class="btn btn-info">
                Details
            </a>
        </li>
    }
</ul>